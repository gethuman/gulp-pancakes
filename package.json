  },
  "dependencies": {
    "through2": "*",
    "pancakes": "0.0.12",
    "lodash": "~2.4.1",
    "q": "^1.0.1"
  },
  "devDependencies": {
    "gulp": "^3.8.9",
    "taste": "^0.1.3"
