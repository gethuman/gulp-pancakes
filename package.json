{
  "name": "gulp-pancakes",
  "version": "0.0.1",
  "description": "Gulp plugin for the Pancakes framework",
  "keywords": [
    "gulpplugin"
  ],
  "homepage": "https://github.com/gethuman/gulp-pancakes",
  "main": "lib/gulp.pancakes.js",
  "scripts": {
    "test": "istanbul test _mocha --report html -- test/unit/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha test/unit --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gethuman/gulp-pancakes.git"
  },
  "author": {
    "name": "Jeff Whelpley",
    "email": "jeff@gethuman.com",
    "url": "https://github.com/jeffwhelpley"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gethuman/gulp-pancakes/issues"
  },
  "dependencies": {
    "through2": "*",
    "gulp-util": "~2.2.0",
    "pancakes": "0.0.2",
    "lodash": "~2.4.1",
    "q": "^1.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.8.0",
    "mocha-lcov-reporter": "*",
    "istanbul": "*",
    "event-stream": "*",
    "mocha": "~1.17.1",
    "chai": "~1.9.0"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.2.10"
  }
}
